<!--
---
****************************
  CONTAINERS
****************************
---
-->
<!-- groupFieldsContainer -->
{% block groupFieldsContainer %}
    <li class="metabox_group">
        <div class="metabox_trigger active">{{ title }}</div>
        <div class="metabox_content">
            {{ fields }}
        </div>
    </li>
{% endblock groupFieldsContainer %}
<!-- groupFieldsContainer -->

<!-- dynamicGroupFieldsContainer -->
{% block dynamicGroupFieldsContainer %}
    <div class="dynamicContainer">
        {% for single in fields %}
            <div class="dynamic_fieldset">
                <div class="dynamic_fieldset_append {{ loop.first == true ? '' : 'delete' }}"></div>
                <div class="dynamic_fieldset_inner">
                    {{ single }}
                </div>
            </div>
        {% endfor %}
    </div>
{% endblock dynamicGroupFieldsContainer %}
<!-- dynamicFieldsContainer -->


<!--
---
****************************
  FIELDS
****************************
---
-->

<!-- hidden -->
{% block hidden %}
    <li>
        {% block hiddenInner %}
            <div class="fieldset clearfix" style="display: none">
                <label>{{ field.title }}</label>
                <label class="full">
                    <input type="hidden" name="{{ field.label }}" value="{{ field.value }}" class="form-control" />
                </label>

                {% if field.options.help is defined %}
                    <div class="alert alert-warning help">
                        {{ field.options.help }}
                    </div>
                {% endif %}
            </div>
        {% endblock hiddenInner %}
    </li>
{% endblock hidden %}
<!-- //hidden -->

<!-- ******************************************************************************************************************************** -->

<!-- text | Static -->
{% block text %}
    <li>
        {% block textInner %}
            <div class="fieldset clearfix">
                <label>{{ field.title }}</label>
                <label class="full">
                    <input type="text" name="{{ field.label }}" value="{{ field.value|escape }}" class="form-control" {{ field.options.limit_counter.limit != '' ? 'data-limit-counter="' ~ field.options.limit_counter.limit ~ '"' : '' }} {{ disableOption }} />
                </label>

                {% if field.options.help is defined %}
                    <div class="alert alert-warning help">
                        {{ field.options.help }}
                    </div>
                {% endif %}

                {% if field.options.limit_counter.limit is defined %}
                    <div class="alert alert-info help counter">
                        {{ field.options.limit_counter.description }} <span class="limit_counter valid">{{ field.options.limit_counter.limit }}</span> karakter kaldı.
                    </div>
                {% endif %}
            </div>
        {% endblock textInner %}
    </li>
{% endblock text %}
<!-- //text | Static -->


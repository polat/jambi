        {% block dateMultipleInner %}
            <div class="fieldset clearfix">
                <label>{{ field.title }}</label>
                <label class="full">
                    <div id="{{ field.label }}" class="multipleDate"></div>
                    <input type="hidden" name="{{ field.label }}" value="{{ field.value }}" />
                </label>

                {% if field.options.help is defined %}
                    <div class="alert alert-warning help">
                        {{ field.options.help }}
                    </div>
                {% endif %}

                <script>
                    $(document).ready(function () {
                        var year = new Date().getFullYear().toString().substr(-2);

                        $('#{{  field.label }}').multiDatesPicker({
                            dateFormat: "dd.mm.yy",

                            {% if not field.value is empty %}
                            addDates: [{{ field.value }}],
                            {% endif %}

                            numberOfMonths: [1,4],
                            defaultDate: year + '-1-1'
                        });
                    })
                </script>
            </div>
        {% endblock dateMultipleInner %}
    </li>
{% endblock dateMultiple %}
